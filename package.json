{
  "name": "kitra-serino",
  "version": "1.0.0",
  "description": "A mini-project treasure game that uses Node.js, Express, Joi (Validation), Jest and SuperTest (Testing) and JavaScript as the primary language.",
  "main": "index.js",
  "scripts": {
    "db:generate": "npx sequelize-cli migration:create --name",
    "db:generate:seed": "npx sequelize-cli seed:generate --name",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:rollback": "npx sequelize-cli db:migrate:undo:all",
    "db:rollback:seed": "npx sequelize-cli db:seed:undo:all",
    "serve": "node index.js"
  },
  "keywords": [],
  "author": "Antonio Aradaza",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "http-status": "^1.7.4",
    "joi": "^17.12.2",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.7"
  }
}
